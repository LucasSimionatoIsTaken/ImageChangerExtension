{
    "name": "ImageChanger",
    "version": "3.1",
    "manifest_version": 3,
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["main.js"]
      }
    ],
    "web_accessible_resources": [
        {
            "resources": ["*.png", "*.jpg"],
            "matches": ["<all_urls>"]
        }
    ]
}
  